{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb/base"],
  "rules": {
    "no-console":"off",
    "no-plusplus":"off",
    "max-len":"off",
    "linebreak-style":"off",
    "func-names":"off",
    "prefer-rest-params":"off",
    "no-restricted-syntax":"off",
    "consistent-return":"off",
    "no-await-in-loop":"off",
    "no-alert":"off"
  }
}